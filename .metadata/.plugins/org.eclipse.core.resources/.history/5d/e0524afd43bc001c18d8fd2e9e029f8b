#include <stdio.h>
#include <stdlib.h>

int validarEsMayor(int numero, int aComparar)
{
	int retorno;

	retorno = 0;

	if(numero > aComparar)
	{
		retorno = 1;
	}

	return retorno;
}

char validarLetra(char letra, char aComparar)
{
	char retorno;

	retorno = 1;

	if(letra != aComparar)
	{
		retorno = 0;
	}

	return retorno;
}


