int validarEsMayor(int numeroUno, int numeroDos)
{
	int retorno;

	retorno = 0;

	if(numeroUno > numeroDos)
	{
		retorno = 1;
	}

	return retorno;
}

char validarLetra(char letraA, char letraB, char letraC)
{
	char retorno;

	retorno = 0;

	if(letraA == letraB || letraA == letraC)
	{
		retorno = 1;
	}

	return retorno;
}


