#include <stdio.h>
#include <stdlib.h>

int validarEsMayor(int numero, int aComparar)
{
	int retorno;

	retorno = 0;

	if(numero > aComparar)
	{
		retorno = 1;
	}

	return retorno;
}

char validarLetra(char letraA, char letraB, char letraC)
{
	char retorno;

	retorno = 1;

	if(letraA != letraB || letraA != letraC)
	{
		retorno = 0;
	}

	return retorno;
}


