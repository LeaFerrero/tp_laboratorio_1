int calcularDescuento(int numero, int porcentaje)
{
	int numeroDescontado;
	int numeroConDescuento;

	numeroDescontado = (numero * 100) / porcentaje;
	numeroConDescuento = numero - numeroDescontado;

	return numeroConDescuento;
}
