#include <stdio.h>
#include <stdlib.h>

#ifndef OPERACIONES_H_
#define OPERACIONES_H_

float sumarFlotante(int numeroUno, int numeroDos);

float restarFlotante(float numeroUno, float numeroDos);

float dividirFlotante(float numeroUno, float numeroDos);

float multilicarFlotante(float numeroUno, float numeroDos);

float calcularDescuento(float, int);

float calcularAumento(float, int);

float pasarAMoneda(float valor, float moneda);

#endif /* OPERACIONES_H_ */
