float calcularDescuento(int numero, int porcentaje)
{
	float numeroDescontado;
	float numeroConDescuento;

	numeroDescontado = (float)(numero * 100) / porcentaje;
	numeroConDescuento = (float)numero - numeroDescontado;

	return numeroConDescuento;
}
